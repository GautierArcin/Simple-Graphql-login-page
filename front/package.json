{
  "name": "minimal-react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "formik": "^2.2.9",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "typeit-react": "^2.6.0",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write './**/*.{js,md,ts,tsx,jsx}'",
    "lint": "eslint --cache --fix --ext './**/*.{js,ts,tsx,jsx}' --format=pretty ./",
    "generate": "graphql-codegen",
    "start-server": "npm start",
    "cy:run": "cypress run",
    "test": "start-server-and-test start-server http://localhost:3000 cy:run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "cypress": "^10.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-pretty": "^2.1.1",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.1.2",
    "msw": "^0.39.2",
    "prettier": "2.5.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --cache --fix --format=pretty ./"
  },
  "msw": {
    "workerDirectory": "public"
  }
